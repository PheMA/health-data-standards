<%- if details[:coded_entry_method] == :ordinality -%>
          <priorityCode>
            <%- if value.code_list_id -%>
            <item valueSet="<%= value.code_list_id %>" />
            <%- end -%>
          </priorityCode>
<%- else -%>
          <outboundRelationship typeCode="<%= details[:type_code].nil? ? 'SUBJ' : details[:type_code] %>">
<%- if details[:code] == 'SDLOC' -%>
            <encounterCriteria classCode="ENC" moodCode="EVN">
              <%- if details[:template_id] -%>
              <templateId>
                <item root="<%= details[:template_id] %>" extension="2014-11-24"/>
              </templateId> 
              <%- end -%>
              <participation typeCode="LOC">
                <role classCode="SDLOC">
                  <%= xml_for_value(value, 'code') %>
                </role>
              </participation>
            </encounterCriteria>
<%- else -%>
            <observationCriteria classCode="OBS" moodCode="EVN">
              <%- if details[:template_id] -%>
              <templateId>
                <item root="<%= details[:template_id] %>" extension="2014-11-24"/>
              </templateId> 
              <%- end -%>
              
              <%- if is_transfer(details[:code]) -%>
              <title value="<%= details[:code] %>" />
              <statusCode code="completed" />
              <%= xml_for_value(value) %>
              <participation  typeCode="ORG">
                <role classCode="LOCE">
                  <code xsi:type="CD" />
                </role>
              </participation>
              <%- else -%>
              <title value="<%= details[:title] %>" />
              <code code="<%= details[:code] %>" codeSystem="<%= details[:code_system] %>" codeSystemName="<%= details[:code_system_name] %>">
                <displayName value="<%=ERB::Util::h details[:title] %>"/> 
              </code>
              <%= xml_for_value(value) %>
              <%- end -%>
            </observationCriteria>
<%- end -%>
          </outboundRelationship>
<%- end -%>